{
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.vite.tsbuildinfo",
    "target": "ES2023",
    "module": "Preserve",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleDetection": "force",
    "verbatimModuleSyntax": true,
    "allowImportingTsExtensions": true
  },
  "include": ["vite.config.ts"]
}
